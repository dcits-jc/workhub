<div class="row">
  <!-- 首部信息 -->
  <div class="col-md-9">


    <!-- 导航栏 -->
    <ol class="breadcrumb">
      <!-- 周开始与结束时间 -->
      <li class="active">
        <small>
          <%= strf(@start_date) %> ~ <%= strf(@end_date) %>  
        </small>
      </li>
    </ol>



    <!-- 模态框 -->
    <%#= render 'dialogforms/project_workflow_dialog.html.erb' %>





    <!-- 本周的 feeds -->
    <%= render 'common/feeds' %>

  </div>


  <!-- 右侧边 -->
  <div class="col-md-3 hidden-xs hidden-sm">
    <%= render 'team_users' %>
    <div class="row">
      <br>
    </div>
    <%= render 'team_history' %>
  </div>

</div>