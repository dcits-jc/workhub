<div class="col-md-12 col-xs-10 col-xs-offset-1">
  <h3 class="text-center">技术人员搜索</h3>
  <!-- 用户搜索 -->
  <%= search_form_for [:wechatpage,@q], html: {class: "navbar-form navbar-left"} do |f|  %>
    <div class="input-group">
      <%= f.search_field :itcode_or_name_or_email_cont, class: "form-control", placeholder: "输入itcode,姓名或邮箱"  %>
      <span class="input-group-btn">
        <%= f.submit '搜索',class: "btn btn-default" %>
      </span>
    </div>
  <% end %>
</div>


